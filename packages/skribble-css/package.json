{
  "name": "skribble-css",
  "version": "0.0.0",
  "description": "A better way to work with typed css",
  "keywords": [
    "skribble",
    "napi",
    "rust",
    "skribble css",
    "css"
  ],
  "repository": "https://github.com/skribbledev/skribble/tree/master/packages/skribble-css",
  "license": "MIT",
  "contributors": [],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./client": {
      "import": "./dist/client.esm.js",
      "require": "./dist/client.cjs.js",
      "types": "./dist/client.d.ts"
    },
    "./client.js": {
      "import": "./dist/client.esm.js",
      "require": "./dist/client.cjs.js",
      "types": "./dist/client.d.ts"
    },
    "./index.js": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./types/*": "./dist/*.d.ts"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.esm.js",
  "types": "./dist/index.d.ts",
  "files": [
    "client",
    "dist"
  ],
  "dependencies": {
    "@node-rs/helper": "^1.3.3",
    "@skribble-css/client": "^0.0.0",
    "tiny-invariant": "^1.2.0",
    "ts-extras": "^0.10.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.30"
  },
  "peerDependencies": {
    "@types/node": "*"
  },
  "peerDependenciesMeta": {
    "@types/node": {
      "optional": true
    }
  },
  "optionalDependencies": {
    "skribble-css-android-arm64": "^0.0.0",
    "skribble-css-android-eabi": "^0.0.0",
    "skribble-css-darwin-arm64": "^0.0.0",
    "skribble-css-darwin-x64": "^0.0.0",
    "skribble-css-freebsd-x64": "^0.0.0",
    "skribble-css-linux-arm-gnueabihf": "^0.0.0",
    "skribble-css-linux-arm64-gnu": "^0.0.0",
    "skribble-css-linux-arm64-musl": "^0.0.0",
    "skribble-css-linux-x64-gnu": "^0.0.0",
    "skribble-css-linux-x64-musl": "^0.0.0",
    "skribble-css-wasm": "^0.0.0",
    "skribble-css-win32-arm64-msvc": "^0.0.0",
    "skribble-css-win32-ia32-msvc": "^0.0.0",
    "skribble-css-win32-x64-msvc": "^0.0.0"
  },
  "engines": {
    "node": ">= 14"
  },
  "publishConfig": {
    "access": "public"
  },
  "monots": {
    "entrypoints": [
      "index.ts",
      "client.ts"
    ],
    "tsconfigs": {
      "src": {
        "compilerOptions": {
          "types": [
            "node"
          ],
          "noEmit": true
        }
      },
      "test": {}
    },
    "mode": "library",
    "addExportsToEntrypoints": true,
    "externalModules": []
  },
  "napi": {
    "name": "skribble_css",
    "triples": {
      "additional": [
        "aarch64-apple-darwin",
        "aarch64-linux-android",
        "aarch64-unknown-linux-gnu",
        "aarch64-unknown-linux-musl",
        "aarch64-pc-windows-msvc",
        "armv7-unknown-linux-gnueabihf",
        "x86_64-unknown-linux-musl",
        "x86_64-unknown-freebsd",
        "i686-pc-windows-msvc",
        "armv7-linux-androideabi"
      ]
    }
  }
}
